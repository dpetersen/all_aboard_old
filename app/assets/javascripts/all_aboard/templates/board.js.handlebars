{{view templateName="all_aboard/templates/navbar"}}

{{outlet slides}}

{{#if slides.length}}
  <ol>
    {{#each slides}}
      <li>
        Slide ID:{{id}}

        <ul>
          {{#each panes}}
            {{{markup}}}
          {{/each}}
        </ul>
      </li>
    {{/each}}
  </ol>
{{else}}
  <h3>You need to add some slides if you want to see something here.</h3>
{{/if}}
