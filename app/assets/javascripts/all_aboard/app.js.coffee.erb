#= require ./store
#= require ./router
#= require_tree ./models
#= require_tree ./controllers
#= require_tree ./views
#= require_tree ./helpers
#= require_tree ./templates

App.AllAboardTemplates = []
<% AllAboard.source_templates.each do |(path, filesystem_path)| %>
App.AllAboardTemplates["<%= path %>"] = "<%= File.read(filesystem_path).gsub('"', '\"') %>"
<% end %>

App.initialize()
